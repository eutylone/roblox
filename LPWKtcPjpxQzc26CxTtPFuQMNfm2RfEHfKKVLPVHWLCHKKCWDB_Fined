Loadstring: loadstring(game:HttpGet('https://raw.githubusercontent.com/eutylone/roblox/main/LPWKtcPjpxQzc26CxTtPFuQMNfm2RfEHfKKVLPVHWLCHKKCWDB_Fined'))()
